$("#bids_products_row").html('<%= j render "bids/bids_products" %>');
<% if @error_message.present? %>
  $("#bid_amount_product_<%= @bid.product_id %>").addClass("is-invalid");
  $("#bid_error_message_product_<%= @bid.product_id %>").next(".invalid-feedback").text("<%= @error_message %>");
<% end %>
